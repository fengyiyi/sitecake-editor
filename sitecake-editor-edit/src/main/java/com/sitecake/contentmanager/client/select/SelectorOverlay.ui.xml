<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder">
	<ui:style>
		.overlay {
			position: fixed;
			top: 0;
			left: 0;
			right: 0;
			bottom: 0;
			cursor: pointer;
			opacity: 0;
			filter: literal("alpha(opacity=0)");
			zoom: 1;
			z-index: 9000000;
		}
	</ui:style>
	<div class="{style.overlay}">
	</div>
</ui:UiBinder>